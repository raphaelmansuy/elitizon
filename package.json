{
  "name": "elitizonweb_new",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:netlify": "npm run build && npm run export",
    "export": "next export",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out",
    "analyze": "ANALYZE=true npm run build",
    "sitemap": "next-sitemap",
    "test:build": "npm run build && npm run start",
    "test:rich-snippets": "node scripts/test-rich-snippets.js",
    "test:contrast": "node scripts/test-color-contrast.js",
    "test:accessibility": "node scripts/test-accessibility.js",
    "validate:seo": "npm run test:rich-snippets",
    "validate:accessibility": "npm run test:contrast && node scripts/validate-accessibility.js",
    "validate:all": "npm run validate:seo && npm run validate:accessibility"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.828.0",
    "@aws-sdk/client-sts": "^3.828.0",
    "@types/dompurify": "^3.0.5",
    "@types/joi": "^17.2.2",
    "@types/jsdom": "^21.1.7",
    "@vercel/speed-insights": "^1.2.0",
    "critters": "^0.0.23",
    "dompurify": "^3.2.6",
    "joi": "^17.13.3",
    "jsdom": "^26.1.0",
    "next": "15.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sharp": "^0.34.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@netlify/plugin-nextjs": "^5.11.2",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^24.0.3",
    "@types/react": "19.1.8",
    "@types/react-dom": "^19.1.6",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "next-sitemap": "^4.2.3",
    "tailwindcss": "^4",
    "typescript": "5.8.3"
  }
}
